    <a href="/board/write">게시글 작성하기</a> <button onclick="upclick()">정렬:추천순</button>
    
    <div>
      <button><</button>
      <% for(let j = 0; j < 5; j++) { %>
        <button><%=gofunc(j+1)%> </button>
        <% } %>
      <button>></button>
  </div>
    
    
    <script>
      function upclick(){
        location.href="/board/up";
      }
    </script>
